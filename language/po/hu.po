#: webtrees/admin_site_logs.php:224 webtrees/admin_site_change.php:244
#, php-format
msgid "From %s to %s"
msgstr "%s-tól %s-ig"

#. I18N: gedcom tag TYPE
#: webtrees/admin_site_logs.php:228 webtrees/admin_site_logs.php:259
#: webtrees/library/WT/Gedcom/Tag.php:376
#: webtrees/includes/functions/functions_print_facts.php:608
#: webtrees/edit_changes.php:175
#: webtrees_tmp/modules_v3_address_report_report.xml.php:8
msgid "Type"
msgstr "Típus"

#: webtrees/admin_site_logs.php:231 webtrees/admin_site_logs.php:260
